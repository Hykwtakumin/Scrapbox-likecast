{
  "manifest_version": 2,
  "name": "Scrapbox-likecast",
  "version": "1.0.0",

  "description": "Scrapboxにいいねボタンを追加する拡張",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://scrapbox.io/*"],
      "css": ["likestyles.css"],
      "js": ["likescript.js"]
    }
  ]
}